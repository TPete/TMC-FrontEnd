<!DOCTYPE html>
<html>
<head>
    <title>
        {% block title %}
            TV
        {% endblock %}
    </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <link rel="icon" type="image/png" href="http://{{ host }}/imgs/favicon.png">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.4/css/bootstrap.min.css" integrity="sha384-2hfp1SzUoho7/TsGGGDaFdsuuDL0LX2hnUp6VkX3CUQ2K4K+xjboZdsXyp4oUHZj" crossorigin="anonymous">
        <link rel="Stylesheet" type="text/css" href="http://{{ host }}/vendor/fortawesome/font-awesome/css/font-awesome.css"/>
        {#<link rel="Stylesheet" type="text/css" href="http://{{ host }}/js/jquery-ui-1.10.3.custom/css/smoothness/jquery-ui-1.10.3.custom.css"/>#}
        {#<link rel="Stylesheet" type="text/css" href="http://{{ host }}/js/jquery.tagit.css"/>#}
        <link rel="Stylesheet" type="text/css" href="http://{{ host }}/bs.css?ver=1"/>
    {% endblock %}
</head>
<body>
<input type="hidden" id="host" value="{{ host }}">
{% block dialogs %}

{% endblock %}
{% block header_bar %}

{% endblock %}
<div class='container-fluid'>
    {% block content %}
        Hier ist nichts
    {% endblock %}
</div>
{% block javascript %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.0.0/jquery.min.js" integrity="sha384-THPy051/pYDQGanwU6poAc/hOdQxjnOEXzbT+OuUAFqNqFjL+4IGLBgCJC3ZOShY" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.2.0/js/tether.min.js" integrity="sha384-Plbmg8JY28KFelvJVai01l8WyZzrYWG825m+cZ0eDDS1f7d/js6ikvy1+X+guPIB" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.4/js/bootstrap.min.js" integrity="sha384-VjEeINv9OSwtWFLAtmc4JCtEJXXBub00gtSnszmspDLCtC0I4z4nqz7rEFbIZLLU" crossorigin="anonymous"></script>
    {#<script type="text/javascript" src='http://{{ host }}/js/jquery-1.10.2.min.js'></script>#}
    {#<script type="text/javascript" src='http://{{ host }}/js/jquery-ui-1.10.3.custom/js/jquery-ui-1.10.3.custom.min.js'></script>#}
    {#<script type="text/javascript" src='http://{{ host }}/js/tag-it.min.js'></script>#}
    <script type="text/javascript" src='http://{{ host }}/main.js?ver=1'></script>
{% endblock %}
</body>
</html>
