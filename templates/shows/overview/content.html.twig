<div class="row show-list">
    {% for data in overview %}
        <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 my-3">
            <div class="card">
                <a href="{{ data['attributes']['folder'] }}" title="{{ data['attributes']['title'] }}">
                    <img class="card-img-top img-fluid" src="{{ data['attributes']['thumbnail'] }}">
                </a>
                <div class="card-body">
                    <a class="show-card-title" href="{{ data['attributes']['folder'] }}" title="{{ data['attributes']['title'] }}">
                        {{ data['attributes']['title'] }}
                    </a>
                </div>
            </div>
        </div>
    {% endfor %}
    <br class="clear">
</div>